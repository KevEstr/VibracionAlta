# Configuración de la aplicación
REACT_APP_NAME=Sistema de Citas
REACT_APP_VERSION=1.0.0

# Configuración de N8N
REACT_APP_N8N_WEBHOOK_URL=https://tu-n8n-instance.com/webhook/reservar-cita

# Configuración de Google Calendar
REACT_APP_GOOGLE_CALENDAR_API_KEY=tu_api_key_aqui
REACT_APP_GOOGLE_CALENDAR_ID=tu_calendar_id@group.calendar.google.com

# Configuración de la API (opcional)
REACT_APP_API_URL=http://localhost:3001/api

# Configuración de notificaciones
REACT_APP_ENABLE_NOTIFICATIONS=true
REACT_APP_NOTIFICATION_DURATION=5000

# Configuración de desarrollo
REACT_APP_DEBUG=true
REACT_APP_MOCK_DATA=true
