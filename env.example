# Configuración de n8n (URLs de producción)
# URL para consultar disponibilidad de citas
REACT_APP_N8N_WEBHOOK_URL=https://vibracionaltacalendario.app.n8n.cloud/webhook/api/dias-disponibles
# URL para agendar citas
REACT_APP_N8N_BOOKING_URL=https://vibracionaltacalendario.app.n8n.cloud/webhook/agendar-cita
# URL para gestionar citas (listar, cancelar, etc.)
REACT_APP_N8N_MANAGE_URL=https://vibracionaltacalendario.app.n8n.cloud/webhook/gestionar-citas

# Configuración de Cloudinary
# Tu cloud name de Cloudinary (reemplaza 'dxhhrqxwm' con el tuyo)
REACT_APP_CLOUDINARY_CLOUD_NAME=dxhhrqxwm
# Upload preset para comprobantes (debes crearlo en Cloudinary)
REACT_APP_CLOUDINARY_UPLOAD_PRESET=vibracion_alta_comprobantes

# Otras configuraciones
REACT_APP_API_BASE_URL=http://localhost:3001